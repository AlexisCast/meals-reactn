{
  "cli": {
    "version": ">= 5.4.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "distribution": "internal"
    },
    "production": {
      "credentialsSource": "local"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "@EXPO_PUBLIC_APPLE_ID",
        "ascAppId": "@EXPO_PUBLIC_ASC_APP_ID",
        "appleTeamId": "@EXPO_PUBLIC_APPLE_TEAM_ID"
      }
    }
  }
}
